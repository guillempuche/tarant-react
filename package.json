{
  "name": "tarant-react-hook",
  "version": "0.1.1",
  "description": "React Hook for the actor model library Tarant",
  "main": "dist/cjs/index.ts",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "start": "vite",
    "format": "prettier \"*.{js,json,md}\" \"{src,examples}/**/*.{js,ts}\" --write && eslint --fix '*.{js,json}' '{src,examples}/**/*.{ts,ts}'"
  },
  "engines": {
    "node": ">=14.18"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "prettier": {
    "useTabs": false,
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "bracketSameLine": false,
    "tabWidth": 2,
    "printWidth": 80
  },
  "dependencies": {
    "tarant": "^3.1.36",
    "use-sync-external-store": "^1.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-transform-react-jsx": "^7.21.0",
    "@babel/plugin-transform-typescript": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/react": "^18.0.37",
    "@types/use-sync-external-store": "^0.0.3",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.8.4",
    "rollup": "^3.19.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "vite": "^4.2.0"
  },
  "contributors": [
    "Guillem Puche (https://github.com/guillempuche)",
    "Kevin Mas Ruiz (https://github.com/kmruiz)"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guillempuche/tarant-react.git"
  },
  "license": "MIT",
  "homepage": "https://github.com/guillempuche/tarant-react#readme",
  "bugs": {
    "url": "https://github.com/guillempuche/tarant-react/issues"
  },
  "keywords": [
    "react",
    "actor model",
    "actor system",
    "actor",
    "tarant",
    "ddd",
    "domain-driven design",
    "event",
    "messaging",
    "state",
    "state management"
  ]
}